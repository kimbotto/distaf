{
  "browsers": "chrome",
  "src": "e2e-tests/**/*.test.ts",
  "reporter": [
    {
      "name": "spec"
    },
    {
      "name": "json",
      "output": "e2e-tests/reports/test-results.json"
    }
  ],
  "screenshots": {
    "path": "e2e-tests/screenshots/",
    "takeOnFails": true,
    "pathPattern": "${DATE}_${TIME}/test-${TEST_INDEX}/${USERAGENT}/${FILE_INDEX}.png"
  },
  "videoPath": "e2e-tests/videos/",
  "videoOptions": {
    "singleFile": false,
    "failedOnly": true
  },
  "concurrency": 1,
  "selectorTimeout": 10000,
  "assertionTimeout": 5000,
  "pageLoadTimeout": 30000,
  "speed": 1,
  "debugOnFail": false,
  "skipJsErrors": false,
  "quarantineMode": false,
  "stopOnFirstFail": false,
  "disablePageReloads": false,
  "color": true
}