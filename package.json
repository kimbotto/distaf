  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --define:process.env.NODE_ENV='\"production\"'",
    "start": "node dist/index.js",
    "check": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "testcafe chrome e2e-tests/",
    "test:e2e:headless": "testcafe chrome:headless e2e-tests/",
    "test:e2e:firefox": "testcafe firefox e2e-tests/",
    "test:e2e:safari": "testcafe safari e2e-tests/",
    "test:e2e:all": "testcafe chrome,firefox,safari e2e-tests/",
    "test:e2e:dev": "testcafe chrome e2e-tests/ --live",
    "test:e2e:videos": "testcafe chrome e2e-tests/ --config-file .testcaferc.with-videos.json",
    "test:e2e:auth": "testcafe chrome e2e-tests/auth.test.ts",
    "test:e2e:assessment": "testcafe chrome e2e-tests/assessment.test.ts",
    "test:e2e:results": "testcafe chrome e2e-tests/results.test.ts",
    "test:e2e:navigation": "testcafe chrome e2e-tests/navigation.test.ts",
    "db:push": "drizzle-kit push",
    "db:reset": "node -e \"require('fs').rmSync('data/distaf.db', {force: true})\" && npm run db:push",
    "db:studio": "drizzle-kit studio",
    "dev:full": "npm run db:push && npm run dev",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:deploy": "node -e \"require('child_process').execSync('bash deploy.sh', {stdio: 'inherit'})\"",
    "docker:prod": "docker compose --env-file .env.production up --build -d"
  },
